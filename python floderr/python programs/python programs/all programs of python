######Fibonacci series
n = int(input())
a, b = 0, 1

for _ in range(n):
    print(a, end=" ")
    a, b = b, a + b
########
###Nth Fibonacci
n = int(input())
a, b = 0, 1

for _ in range(n):
    a, b = b, a + b

print(a)



#####Compound Interest
p = float(input())
r = float(input())
t = float(input())

ci = p * ((1 + r/100)**t) - p
print(ci)


####. BMI
weight = float(input())
height = float(input())  # in meters
bmi = weight / (height * height)
print(bmi)



#Speed = Distance / Time
d = float(input())
t = float(input())
speed = d / t
print(speed)

####Km to miles







#####
#Average of N float numbers
n = int(input())
s = 0

for i in range(n):
    x = float(input())
    s += x

print(s / n)


#####Count consonants
s = input()
vowels = "aeiouAEIOU"
count = 0
for ch in s:
    if ch.isalpha() and ch not in vowels:
        count += 1
print(count)


####. Reverse string without slicing
s = input()
rev = ""
for ch in s:
    rev = ch + rev
print(rev)




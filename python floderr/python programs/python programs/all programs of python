#Positive, Negative, or Zero
n = int(input())
if n > 0:
    print("Positive")
elif n < 0:
    print("Negative")
else:
    print("Zero")
##########
#####number palindrome
n = int(input())
temp = n
rev = 0
while temp > 0:
    rev = rev * 10 + temp % 10
    temp //= 10

print("Palindrome" if rev == n else "Not Palindrome")
######armstrong number
n = int(input())
temp = n
s = 0

while temp > 0:
    digit = temp % 10
    s += digit ** 3
    temp //= 10

print("Armstrong" if s == n else "Not Armstrong")
######factorial
n = int(input())
fact = 1
for i in range(1, n+1):
    fact *= i
print(fact)

#####GCD
a = int(input())
b = int(input())

while b != 0:
    a, b = b, a % b

print(a)
#############
####LCM
a = int(input())
b = int(input())

x, y = a, b
while b != 0:
    a, b = b, a % b

gcd = a
lcm = (x * y) // gcd
print(lcm)
